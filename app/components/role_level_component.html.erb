<div>
  <h2 class="text-sm font-medium text-gray-500"><%= t(".interested_in_role_levels") %></h2>
  <ul role="list" class="mt-3 space-y-3">
    <% selected_role_levels.each do |selected_role_level| %>
      <li class="flex items-center space-x-2">
        <%= inline_svg_tag "icons/solid/check_circle.svg", class: "h-5 w-5 text-gray-400" %>
        <span class="text-gray-900 text-sm font-medium"><%= humanize(selected_role_level) %></span>
      </li>
    <% end %>
  </ul>
</div>
